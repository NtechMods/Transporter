			<PrimarySound>Transporter</PrimarySound>
			<BuildTimeSeconds>90</BuildTimeSeconds>
			<DamageEffectId>214</DamageEffectId>
			<DamagedSound>ParticleElectrical</DamagedSound>
			<PCU>600</PCU>
